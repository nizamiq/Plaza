{
  "name": "@nizamiq/plaza",
  "version": "0.1.0",
  "description": "Plaza - MCP Tooling Platform: Universal Scraper, Browser Automation, Web Search",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./universal-scraper": {
      "import": "./dist/universal-scraper/index.js",
      "types": "./dist/universal-scraper/index.d.ts"
    },
    "./browser-automation": {
      "import": "./dist/browser-automation/index.js",
      "types": "./dist/browser-automation/index.d.ts"
    },
    "./web-search": {
      "import": "./dist/web-search/index.js",
      "types": "./dist/web-search/index.d.ts"
    }
  },
  "bin": {
    "plaza": "./dist/index.js",
    "plaza-mcp-server": "./dist/mcp-server.js",
    "plaza-api-server": "./dist/api-server.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "scraper",
    "browser",
    "automation",
    "web-search",
    "playwright",
    "cheerio"
  ],
  "author": "NizamIQ",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.27.0",
    "axios": "^1.13.5",
    "bottleneck": "^2.19.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "p-retry": "^6.0.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.40.0",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yaml": "^1.9.6",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
